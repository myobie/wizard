<ol>
  <%= for event <- @events do %>
    <%= if event.type == "file.update" do %>
      <li data-id="<%= event.id %>">
        <%= format_actors(event.actors) %>
        updated a file
        <%= format_file_name(event) %>

        <%= for preview <- event.previews do %>
          <p>
            <img src="<%= preview_path @conn, :preview, preview.id %>">
          </p>
        <% end %>
      </li>
    <% end %>
  <% end %>
</ol>
